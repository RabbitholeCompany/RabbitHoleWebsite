---
import Layout from "~/layouts/PageLayout.astro";

import Header from "~/components/widgets/Header.astro";
import { headerData } from "~/data";
import InvestList from "~/components/widgets/InvestList.astro";

const meta = {
  title: "투자 유치",
};
---

<Layout {meta}>
  <Fragment slot="header">
    <Header {...headerData} isSticky />
  </Fragment>

  <InvestList
    title="투자 유치 현황"
    subtitle="래빗홀 컴퍼니의 소중한 인연들입니다."
    items={[
      {
        title: "많은 투자 바랍니다.",
        description: "투자 대기 중",
      },
    ]}
  />
</Layout>
